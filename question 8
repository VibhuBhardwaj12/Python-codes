n=int(input("Enter the no. of elements"))
cubes=[]
for i in range(n):
    list1=int(input("Enter the elements of the list"))
    cubes.append(list1)
print("The list is",cubes)
list2=[]

for j in cubes:
    if type(j)==int and j%2==0:
        list2.append(j**3)
    else:
        list2.append(i)
print(list2)
